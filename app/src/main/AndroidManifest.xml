<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="bjpkten.activitytest">

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activitys.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activitys.SecondActivity" />
        <activity android:name=".activitys.BundlerActivity" />
        <activity android:name=".activitys.ApplicationActivity" />
        <activity android:name=".activitys.SharePreferenceActivity" />
        <activity android:name=".activitys.DBActivity" />
        <activity android:name=".activitys.IntentAcitivity" />
        <activity android:name=".activitys.StaticActivity"></activity>
    </application>

</manifest>